<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <listeners>
        <listener class-name="SeleniumFramework.utils.ExtentReportListener" />
        <listener class-name="SeleniumFramework.utils.SuiteListener"></listener>
    </listeners>
    <test thread-count="5" name="ChromeTest">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="FirstSuite.loginTest"></class>
            <class name="FirstSuite.dynElemTest"></class>
            <class name="FirstSuite.dropTest"></class>
        </classes>
    </test> <!--Test -->
</suite> <!--Suite-->